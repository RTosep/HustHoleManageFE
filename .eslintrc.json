{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "class-methods-use-this": "off",
        "prefer-const": "off",
        "no-unused-vars": "off"
    }
}
